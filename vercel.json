{
    "version": 2,
    "builds": [
        {
            "src": "webpack.config.js",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "buildCommand": "npx webpack --mode production"
}