{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "buildCommand": "npm run vercel-build",
                "outputDirectory": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/models/(.*)",
            "dest": "/models/$1"
        },
        {
            "src": "/textures/(.*)",
            "dest": "/textures/$1"
        },
        {
            "src": "/sounds/(.*)",
            "dest": "/sounds/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}