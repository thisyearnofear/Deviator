{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "buildCommand": "npm run build",
                "outputDirectory": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            },
            "dest": "/assets/$1"
        },
        {
            "src": "/(.*)\\.js",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            },
            "dest": "/$1.js"
        },
        {
            "src": "/(.*)\\.map",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            },
            "dest": "/$1.map"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/.*",
            "dest": "/index.html"
        }
    ]
}